{
    "name": "local/github",
    "license": "MIT",
    "type": "library",
    "description": "Simple GitHub integration library",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/cache": "^5.4|^6.0",
        "php-http/guzzle7-adapter": "^1.0",
        "knplabs/github-api": "^3.11",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "nesbot/carbon": "^2.69"
    },
    "autoload": {
        "psr-4": {
            "App\\GitHub\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
