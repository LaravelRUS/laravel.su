{
    "name": "local/github",
    "license": "MIT",
    "type": "library",
    "description": "Simple GitHub integration library",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/cache": "^5.4|^6.0",
        "php-http/guzzle7-adapter": "^1.0",
        "knplabs/github-api": "^3.6",
        "illuminate/support": "^8.0|^9.0",
        "nesbot/carbon": "^2.57"
    },
    "autoload": {
        "psr-4": {
            "App\\GitHub\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "App\\GitHub\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
